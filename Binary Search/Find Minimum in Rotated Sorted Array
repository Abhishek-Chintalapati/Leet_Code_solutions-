class Solution:
    def findMin(self, nums: List[int]) -> int:
        len_arr = len(nums)
        l = 0
        r = len_arr - 1

        while l < r:
            m = (l + r) // 2

            if nums[m] > nums[r]:
                l = m + 1
            elif nums[m] < nums[r]:
                r = m
            else:
                r -= 1

        return nums[l]

        
